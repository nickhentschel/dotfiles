{
    "editor.fontFamily": "JetBrains Mono, Menlo, Monaco, 'Courier New', monospace",
    "editor.cursorStyle": "block",
    "editor.tabCompletion": "on",
    "editor.cursorBlinking": "smooth",
    "editor.minimap.enabled": false,
    "editor.formatOnSave": false,
    "workbench.startupEditor": "none",
    "editor.fontSize": 14,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.settings.useSplitJSON": true,
    "zenMode.fullScreen": false,
    "window.titleBarStyle": "native",
    "explorer.confirmDragAndDrop": false,
    "explorer.incrementalNaming": "smart",
    "debug.console.fontFamily": "JetBrains Mono",
    "debug.console.fontSize": 14,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontFamily": "JetBrains Mono",
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.zoomLevel": 0,
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "vs-kubernetes": {
        "vs-kubernetes.minikube-path.mac": "/Users/nhentschel/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vscode-kubernetes.minikube-path.mac": "/Users/nhentschel/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
    "explorer.confirmDelete": false,
    "[markdown]": {
        "editor.defaultFormatter": "mrmlnc.vscode-remark"
    },
    "git.autofetch": true,
    "cSpell.userWords": [
        "Burstable",
        "Kubernetes",
        "Wayfair",
        "performant"
    ],
    "vscode-neovim.neovimExecutablePaths.darwin": "/opt/local/bin/nvim",
    "workbench.colorTheme": "Solarized Dark",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.gpuAcceleration": "on",
    "python.showStartPage": false,
    "go.toolsManagement.autoUpdate": true
