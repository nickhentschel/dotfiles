{
    "editor.fontFamily": "JetBrains Mono, Menlo, Monaco, 'Courier New', monospace",
    "editor.cursorStyle": "block",
    "editor.tabCompletion": "on",
    "editor.cursorBlinking": "smooth",
    "editor.minimap.enabled": false,
    "editor.formatOnSave": false,
    "workbench.startupEditor": "none",
    "editor.fontSize": 14,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.settings.useSplitJSON": true,
    "zenMode.fullScreen": false,
    "window.titleBarStyle": "native",
    "explorer.confirmDragAndDrop": false,
    "explorer.incrementalNaming": "smart",
    "debug.console.fontFamily": "JetBrains Mono",
    "debug.console.fontSize": 14,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontFamily": "JetBrains Mono",
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "window.zoomLevel": 0,
    "[python]": {
        "editor.defaultFormatter": null
    },
    "vs-kubernetes": {
        "vs-kubernetes.minikube-path.mac": "~/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vscode-kubernetes.minikube-path.mac": "~/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
    "explorer.confirmDelete": false,
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "git.autofetch": true,
    "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
    "workbench.colorTheme": "One Dark Pro",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.gpuAcceleration": "on",
    "go.toolsManagement.autoUpdate": true,
    "yaml.schemas": {
        "./context.schema.json": "context.yaml"
    },
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "redhat.telemetry.enabled": false,
    "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/vscode.vim",
    "vscode-neovim.compositeKeys": {
        "jk": {
            "command": "vscode-neovim.escape"
        },
        "gq": {
            "command": "rewrap.rewrapComment",
            "modes": [
                "visual"
            ]
        }
    },
    "vscode-neovim.compositeTimeout": 200,
    "npm.exclude": "",
    "editor.lineNumbers": "on",
    "editor.rulers": [
        80
    ],
    "editor.scrollBeyondLastLine": false,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "editor.autoIndent": "full",
    "files.trimTrailingWhitespace": true,
    "editor.wordWrap": "off",
    "workbench.iconTheme": "material-icon-theme",
    "json.format.enable": true,
    "zillowMcp.versionInfo": "Current: 0.1.17 | Latest: 0.1.17",
    "zillowMcp.authStatusOkta": "Authenticated as Nick Hentschel",
    "zillowMcp.authStatusFigma": "Not authenticated",
    "zillowMcp.authStatusGitLab": "Not authenticated",
    "zillowMcp.recheckVersion": false,
    "zillowMcp.enableZillowDocsMCP": true,
    "zillowMcp.enableAtlassianMCP": true,
    "cursor.cpp.disabledLanguages": []
}